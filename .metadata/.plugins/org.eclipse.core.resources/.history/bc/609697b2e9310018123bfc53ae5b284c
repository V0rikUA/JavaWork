package com.gmail.kirovdmitry;

public class Main {

	public static void main(String[] args) {
		Car carOne = new Car();
		carOne.color = "Blue";
		carOne.weight = 2000;
		carOne.year = 1990;
		carOne.beep();
		carOne.print();

		System.out.println();

		Car carTwo = new Car("Red", 3500, 2010);
		carTwo.print();
	}

}
